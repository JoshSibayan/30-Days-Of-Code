        System.out.print("5\n4 3\n0 1 -1 2\n5 2\n0 1 -1 -2 -3\n9 6\n0 1 -1 2 3 4 5 6 7\n7 5\n0 1 -1 -2 -3 -4 -5\n8 4\n0 1 -1 2 3 4 5 6");
    }
    
}
